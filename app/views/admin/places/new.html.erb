<div id="sup_edit_stop" class="container">
  <div class="inner">
    <h2>Add a Place:</h2>
    <div class="card no-click wide-btn">
      <%= form_for [:admin, @place] do |form| %>
        <div class="inner">
          <%= render partial: 'fields', locals: {form: form} %>
        </div>
        <%= form.submit "Create Place", :class => "button name_validation" %>
      <% end %>
    </div>
  </div>
</div>
