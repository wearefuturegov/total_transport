%section.section
  .card
    %h2
      Timetables
      
    %table.table
      %thead
        %tr
          %th Route
          %th Start Date
          %th End Date
          %th
      %tbody
        = render partial: 'timetable', collection: @timetables
    
    .action.primary
      = link_to 'Create new Timetable', new_admin_timetable_path, { class: 'button' }
