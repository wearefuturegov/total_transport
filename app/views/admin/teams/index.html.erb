<div class="container">
  <div class="inner">
    <div class="card">
      <table>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th></th>
        </tr>
        <% @teams.each do |team| %>
          <tr>
            <td><%= team.name %></td>
            <td><%= team.email %></td>
            <td><%= link_to "edit", edit_admin_team_path(team) %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
